<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.techmoresupplies">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/techmore_supply_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/techmore_supply_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.TechMoreSupplies"
        tools:targetApi="31">
        <activity
            android:name=".activity_forgot_password"
            android:exported="false" />
        <activity
            android:name=".activity_cart"
            android:exported="false" />
        <activity
            android:name=".activity_add_to_cart"
            android:exported="false" />
        <activity
            android:name=".activity_products"
            android:exported="false" />
        <activity
            android:name=".activity_splash"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity_login" />
        <activity android:name=".activity_signup" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>